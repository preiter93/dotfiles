{
    "rust-test": {
      "prefix": "test",
      "description": "Rust test module",
      "body": [
        "#[cfg(test)]",
        "mod tests {",
        "    use super::*;",
        "",
        "    #[test]",
        "    fn ${1:test_name}() {",
        "        let result = ${2:your_function}(${3:args});",
        "        assert_eq!(result, ${4:expected});",
        "    }",
        "}"
      ]
    }
}
